    <uses-permission android:name="com.sec.android.provider.badge.permission.READ" /> <!-- 魅族的权限  配置结束 -->
    <!-- 小米的权限  配置开始 -->
    <uses-permission android:name="com.sec.android.provider.badge.permission.WRITE" /> <!-- 这里com.xiaomi.mipushdemo改成app的包名 -->
    <uses-permission android:name="com.meizu.flyme.push.permission.RECEIVE" /> <!-- 这里com.xiaomi.mipushdemo改成app的包名 -->
    <!-- 小米的权限  配置结束 -->
    <!-- oppo的权限  配置开始 -->
    <uses-permission android:name="xin.banghua.beiyuan.push.permission.MESSAGE" />
    <uses-permission android:name="com.meizu.c2dm.permission.RECEIVE" />
    <uses-permission android:name="xin.banghua.beiyuan.permission.C2D_MESSAGE" />
    <uses-permission android:name="xin.banghua.beiyuan.permission.MIPUSH_RECEIVE" />
    <uses-permission android:name="com.coloros.mcs.permission.RECIEVE_MCS_MESSAGE" />
    <uses-permission android:name="com.heytap.mcs.permission.RECIEVE_MCS_MESSAGE" /> <!-- 穿山甲 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" /> <!-- 必要权限，解决安全风险漏洞，发送和注册广播事件需要调用带有传递权限的接口 -->
    <uses-permission
